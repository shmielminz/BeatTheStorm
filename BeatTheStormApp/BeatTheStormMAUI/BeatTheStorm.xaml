             x:Class="BeatTheStormMAUI.BeatTheStorm"
             Title="BeatTheStorm">
    <ContentPage.Resources>
        <x:String x:Key="help">&#x3f;</x:String>
        <x:String x:Key="undo">&#xf2ea;</x:String>
        <x:String x:Key="redo">&#xf2f9;</x:String>
        <x:String x:Key="more">&#xf0d7;</x:String>
